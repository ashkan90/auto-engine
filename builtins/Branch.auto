
define Branch {
    @inputs(
        exec: (label: 'Exec Label', type: @Exec),
        condition: (label: 'Condition', type: @Bool)
    )
    @outputs(
        exec: (label: 'Exec Label', type: @Exec),
        true: (label: 'True Label', type: @Exec),
        false: (label: 'False Label', type: @Exec)
    )
}